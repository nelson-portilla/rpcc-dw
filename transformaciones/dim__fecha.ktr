					date_MMdd == '1016' ||
					date_MMdd == '1106' ||
					date_MMdd == '1113' ||
					date_MMdd == '1208' ||
					date_MMdd == '1225'
					)? "t":"f";

var name_holiday="Ordinario";

switch(date_MMdd.toString()){
 	case "0101":
 		 name_holiday="New Year";
		 break;
	case "0109":
 		 name_holiday="Dia de los Reyes Magos";
		 break;
	case "0320":
