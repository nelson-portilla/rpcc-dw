        <jsScript_script>//Script here

var Calendar = Packages.java.util.Calendar;
var Locale = Packages.java.util.Locale;
var SimpleDateFormat = Packages.java.text.SimpleDateFormat;

var year_for_week = (week_of_year == 1 &amp;&amp; month==12)?year+1:year;
var quarter = Math.floor((month-1) / 3) +1;
var cal = Calendar.getInstance();
cal.set(year, month-1, day);
var d = cal.getTime();

var hoy = new java.util.Date();

// generate labels for months and weeks of day in spanish
var day_of_week_short_label_eng = new SimpleDateFormat("E", new Locale("es","ES")).format(d);
var day_of_week_long_label_eng = new SimpleDateFormat("EEEEE",new Locale("es","ES")).format(d);
var month_short_label_eng = new SimpleDateFormat("MMM",new Locale("es","ES")).format(d);
var month_long_label_eng = new SimpleDateFormat("MMMMM",new Locale("es","ES")).format(d);

// generate amount of days in current month and quarter
var days_in_month = cal.getActualMaximum(Calendar.DAY_OF_MONTH);
var days_in_year = cal.getActualMaximum(Calendar.DAY_OF_YEAR);

var days_in_quarter;

switch(quarter){

	case 1:
		days_in_quarter = (days_in_year == 366)? 31+29+31: 31+28+31; // jan, feb, mar
		break;
	case 2:
		days_in_quarter = 30+31+30;  // apr, may, jun
		break;
	case 3:
		days_in_quarter = 31+31+30;	 // jul, aug, sep
		break;
	case 4:
		days_in_quarter = 31+30+31;	 // oct, nov, dec
		break;
}

var season = "";
var date_MMdd_str = date_MMdd.toString();

if(date_MMdd_str.substr(0,1) == "0"){
	date_MMdd_str = date_MMdd_str.substr(1,3);

}

var date_MMdd_int = parseInt(date_MMdd_str);

if (date_MMdd_int >= 101 &amp;&amp; date_MMdd_int &lt; 320){ 
		season = "Invierno";
	}else if (date_MMdd_int >= 320 &amp;&amp; date_MMdd_int &lt; 620){
		season = "Primavera";
	}else if(date_MMdd_int >= 620 &amp;&amp; date_MMdd_int &lt; 922){
		season = "Verano";
	}else if (date_MMdd_int >= 922 &amp;&amp; date_MMdd_int &lt; 1221){
		season = "Otonio";
	}else if (date_MMdd_int >= 1221 &amp;&amp; date_MMdd_int &lt;= 1231){ 
		season = "Invierno";
	}

// gnerate some flags
var is_leap_year = (days_in_year == 366)?"t":"f";
var is_weekend = (day_of_week == Calendar.SUNDAY || day_of_week == Calendar.SATURDAY)?"t":"f";

var is_holiday = (date_MMdd == '0101' || 
					date_MMdd == '0109' ||
					date_MMdd == '0320' ||
					date_MMdd == '0409' ||
					date_MMdd == '0413' ||
					date_MMdd == '0414' ||
					date_MMdd == '0416' ||
					date_MMdd == '0501' ||
